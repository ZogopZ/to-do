<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
     android:ordering="sequentially">
    <set>
        <objectAnimator
                android:propertyName="rotation"
                android:valueFrom="0"
                android:valueTo="5"
                android:interpolator="@android:interpolator/linear"
                android:duration="35"/>
    </set>
    <set>
        <objectAnimator
                android:propertyName="rotation"
                android:valueFrom="5"
                android:valueTo="-5"
                android:interpolator="@android:interpolator/linear"
                android:duration="70"/>
    </set>
    <set>
        <objectAnimator
                android:propertyName="rotation"
                android:valueFrom="-5"
                android:valueTo="5"
                android:interpolator="@android:interpolator/linear"
                android:duration="70"/>
    </set>
    <set>
        <objectAnimator
                android:propertyName="rotation"
                android:valueFrom="5"
                android:valueTo="0"
                android:interpolator="@android:interpolator/linear"
                android:duration="35"/>
    </set>
</set>